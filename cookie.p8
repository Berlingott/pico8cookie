-- functions and controls
function plyr_update()
	--physics
	plyr.y+=grav
	
	if btn(⬅️) then
		plyr.dx-=plyr.acc
		plyr.running=true
		plyr.flip=true
	end
	if btn(➡️) then
		plyr.dx+=plyr.acc
		plyr.running=true
		plyr.flip=false
	end
	
	--running
	if plyr.running
	and not btn(⬅️)
	and not btn(➡️)
	and not plyr.falling
	and not plyr.jumping then
		plyr.running=false
	end
	
	--jumping 
	if btnp(❎) 
	and plyr.landed then 
		plyr.dy-=plyr.boost
		plyr.landed=false
	end
	
	--check player collision up and down
	if plyr.dy>0 then
		plyr.falling=true
		plyr.landed=false
		plyr.jumping=false

	
		if collide_map(plyr,"down",0) then
			plyr.landed="true"
			plyr.falling="false"
			plyr.dy=0
			plyr.y-=(plyr.y+plyr.h)%8
		end
	elseif plyr.dy<0 then
		plyr.jumping="true"
		if collide_map(plyr,"up",1) then
			plyr.dy=0
		end
	end
		
	
end
