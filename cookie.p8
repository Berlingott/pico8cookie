--init
function _init()
	grav=3

	plyr={--table 
		sp=0, --sp=sprite
		x=48,
		y=32,
		w=4,--with
		h=8,--height
		flp=false,
		dx=0,--delta movement
		dy=0,-- delta movement
		max_dx=3,
		max_dy=3,
		acc_mvmt=0.5,
		acc_jmp=4,
		anim=0,
		acc=0,
		jumping=false,
		falling=false,
		running=false,
		landed=false,
		boost=7
	}
end


-->8
--update and draw
function _update()
		plyr_update()
--		plyr_animate()
end

function _draw()
	cls(13)
	map(0,0)
	spr(plyr.sp,plyr.x,plyr.y,1,1,plyr.flp)
end
